<h4 class="title" style="text-align: center;font-size: xx-large;">USERS</h4>
<div class="container">
  <% @users.each do |user| %>
    <div class="card">
      <div class="card-body">
        <h4 class="card-title"><%= link_to user.email, profile_path(user),class: "sub-heading" %></h4>
      </div> 
    </div>
    <br><br>
  <% end %>
</div>